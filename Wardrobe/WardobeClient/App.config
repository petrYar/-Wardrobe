<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAccountContract" />
                <binding name="BasicHttpBinding_IWardobeContract" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Authorization"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAccountContract"
                contract="Proxy.IAccountContract" name="BasicHttpBinding_IAccountContract" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Wardobe"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWardobeContract"
                contract="Proxy.IWardobeContract" name="BasicHttpBinding_IWardobeContract" />
        </client>
    </system.serviceModel>
</configuration>